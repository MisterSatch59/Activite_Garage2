<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.garage.vue.Fenetre">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Vehicules">
            <items>
              <MenuItem onAction="#voirVehicules" text="Voir" />
                  <MenuItem mnemonicParsing="false" onAction="#ajouterVehicules" text="Ajouter" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Marques">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#voirMarques" text="Voir" />
                  <MenuItem mnemonicParsing="false" onAction="#ajouterMarques" text="Ajouter" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Moteurs">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#voirMoteurs" text="Voir" />
                  <MenuItem mnemonicParsing="false" onAction="#ajouterMoteurs" text="Ajouter" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Options">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#voirOptions" text="Voir" />
                  <MenuItem mnemonicParsing="false" onAction="#ajouterOptions" text="Ajouter" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Types de moteur">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#voirTypesMoteur" text="Voir" />
                  <MenuItem mnemonicParsing="false" onAction="#ajouterTypesMoteur" text="Ajouter" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
